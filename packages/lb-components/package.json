{
  "name": "@labelbee/lb-components",
  "version": "1.8.0",
  "description": "Provide a complete library of annotation components",
  "main": "./dist/index.js",
  "module": "./es/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "scripts": {
    "dev": "npm run build && cross-env NODE_ENV=developemnt rollup -m -c rollup.config.js -w",
    "start": "npm run dev",
    "build": "rm -fr dist && cross-env NODE_ENV=production rollup -c rollup.config.js && npm run build:type",
    "build:type": "tsc --emitDeclarationOnly",
    "commit": "git-cz",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}' && npm run build:type",
    "lint:fix": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "prepublishOnly": "npm run build"
  },
  "author": "laoluo",
  "contributors": [
    "lijingchi",
    "tanjunbao",
    "chenlu",
    "lihuaqi"
  ],
  "keywords": [
    "annotation",
    "canvas",
    "react"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/open-mmlab/labelbee/tree/main/packages/lb-components",
  "bugs": {
    "url": "https://github.com/open-mmlab/labelbee/issues",
    "email": "ron.f.luo@gmail.com"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@labelbee/lb-annotation": "^1.12.0",
    "@labelbee/lb-utils": "^1.5.0",
    "ahooks": "^3.4.0",
    "classnames": "^2.3.0",
    "lodash": "^4.17.21",
    "postcss-url": "^10.1.3",
    "react-i18next": "^11.12.0",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "resize-observer-polyfill": "^1.5.1"
  },
  "peerDependencies": {
    "antd": ">=4.15.0",
    "react": ">=16.9.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/react": "^16.13.1",
    "@types/react-redux": "^7.1.16",
    "antd": "^4.15.0",
    "sass": "^1.53.0"
  }
}
