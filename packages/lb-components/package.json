{
  "name": "@labelbee/lb-components",
  "version": "1.13.0",
  "description": "Provide a complete library of annotation components",
  "main": "./dist/index.js",
  "es": "./es/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "scripts": {
    "dev": "npm run build && cross-env NODE_ENV=developemnt rollup -m -c rollup.config.js -w",
    "start": "npm run dev",
    "build": "rm -fr dist && cross-env NODE_ENV=production rollup -c rollup.config.js && npm run build:type",
    "build:type": "tsc --emitDeclarationOnly",
    "commit": "git-cz",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx && npm run build:type",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "author": "laoluo",
  "contributors": [
    "lijingchi",
    "tanjunbao",
    "chenlu",
    "lihuaqi"
  ],
  "keywords": [
    "annotation",
    "canvas",
    "react"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/open-mmlab/labelbee/tree/main/packages/lb-components",
  "bugs": {
    "url": "https://github.com/open-mmlab/labelbee/issues",
    "email": "brady_luo.sz@foxmail.com"
  },
  "dependencies": {
    "@aas/wavesurfer.js": "^0.0.4-alpha",
    "@ant-design/icons": "^4.6.2",
    "@labelbee/lb-annotation": "1.17.0",
    "@labelbee/lb-utils": "1.9.0",
    "@types/react-dom": "^18.2.7",
    "ahooks": "^3.4.0",
    "classnames": "^2.3.0",
    "decimal.js": "^10.4.3",
    "lodash": "^4.17.21",
    "postcss-url": "^10.1.3",
    "react-colorful": "^5.6.1",
    "react-i18next": "^11.12.0",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "resize-observer-polyfill": "^1.5.1"
  },
  "peerDependencies": {
    "antd": ">=4.15.0",
    "react": ">=16.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@types/lodash": "^4.14.168",
    "@types/react": "^16.13.1",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "antd": "^4.15.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.11.0",
    "eslint": "7.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-alloy": "4.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
